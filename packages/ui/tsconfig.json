{
  "extends": "tsconfig/vue.json",
  "include": ["**/*.ts", "**/*.tsx", "**/*.vue"],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
