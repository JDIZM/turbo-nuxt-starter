# Environment
NODE_ENV=development

# PostgreSQL Database
POSTGRES_DB=starter_db
POSTGRES_USER=starter
POSTGRES_PASSWORD=starter_password
POSTGRES_PORT=5432

# Database URL for applications
DATABASE_URL=postgresql://starter:starter_password@localhost:5432/starter_db

# Application Ports
NUXT_PORT=3001
API_PORT=4000
NITRO_PORT=3000
DOCS_PORT=3002
STORYBOOK_PORT=6006

# API Base URL (for Nuxt frontend)
NUXT_PUBLIC_API_BASE=http://localhost:4000

# Logging
LOG_LEVEL=info
