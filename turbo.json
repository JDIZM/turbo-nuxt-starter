{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".nuxt/**", ".output/**", "dist/**"]
    },
    "lint": {},
    "lint:check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test:unit": {
      "dependsOn": ["^test:unit"],
      "outputs": ["coverage/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "prepare": {
      "cache": false
    },
    "start": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
