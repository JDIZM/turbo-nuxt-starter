{
  "name": "turbo-nuxt",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "lint:check": "turbo run lint:check",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,vue}\"",
    "test:unit": "turbo run test:unit",
    "typecheck": "turbo run typecheck",
    "dev:api": "turbo run dev --filter=api",
    "dev:web": "turbo run dev --filter=nuxt"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsx": "^4.0.0",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20"
  }
}
