{
  "name": "nuxt",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3001",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "nuxt prepare",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,vue}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md,vue}\"",
    "test:unit": "vitest --run --coverage",
    "typecheck": "pnpm exec nuxt typecheck"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@nuxt/image": "^1.7.0",
    "@pinia/nuxt": "^0.5.1",
    "ipx": "^3.0.1",
    "pinia": "^2.1.7",
    "ui": "workspace:*",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@pinia/testing": "^0.1.3",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/vue": "^8.0.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vueuse/core": "^10.4.1",
    "eslint": "^9.0.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^9.0.0",
    "jsdom": "^24.1.0",
    "nuxt": "^4.1.2",
    "prettier": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^4.1.14",
    "tsconfig": "workspace:*",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.6",
    "vue-router": "^4.3.0"
  }
}
